{
  "name": "opencode-nanobanana",
  "version": "0.3.0",
  "description": "OpenCode plugin for visual AI: FREE image generation (Nano Banana / Gemini Flash), video generation (Veo 3), storyboards powered by Google Gemini",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "opencode",
    "mcp",
    "plugin",
    "gemini",
    "veo",
    "imagen",
    "video-generation",
    "image-generation",
    "ai",
    "visual-toolkit",
    "app-icons",
    "storyboard"
  ],
  "author": "NautCoder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/48Nauts-Operator/opencode-nanobanana.git"
  },
  "bugs": {
    "url": "https://github.com/48Nauts-Operator/opencode-nanobanana/issues"
  },
  "homepage": "https://github.com/48Nauts-Operator/opencode-nanobanana#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.36.0",
    "@opencode-ai/plugin": "^1.1.25",
    "sharp": "^0.33.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
